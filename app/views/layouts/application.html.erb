<!DOCTYPE html>
<html>
<head>
  <title>HwBlog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="flash">
		<%= flash[:notice] if flash[:notice] %> 
		<%= flash[:alert] if flash[:alert] %>
	</div>
		
	<%= yield %>

</body>
</html>
